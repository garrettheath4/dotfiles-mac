#!/usr/bin/env bash

git pull || exit 1

if [ "$(git for-each-ref --format="%(if)%(HEAD)%(then)%(push:track)%(end)" refs/heads)" != '' ]; then
	git push || exit 2
	if [ "$(git for-each-ref --format="%(push:track)" refs/heads)" != '' ]; then
		echo 'Other branches:'
		ggu
	fi
else
	echo "Nothing to push on this branch."
fi
